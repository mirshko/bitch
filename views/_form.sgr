form.form(name="hey" netlify netlify-honeypot="mr-roboto" action="cheers")
  .field.-hidden
    input(name="mr-roboto")
  .field
    label Name
    input(type="text" name="name" required)
  .field
    label Email
    input(type="email" name="email" required)
  .field
    label Message
    textarea(name="message" required)
  .field.-btn
    button.btn.-expand(type="submit") Send
